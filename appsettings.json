{
    "Processing": {
        "BatchSize": 10000,
        "MaxMemoryBufferMB": 500,
        "WarmupBars": 200,
        "Timeframes": [ "00:01:00", "00:05:00" ],
        "LabelConfig": {
            "TriggerPips": 3.5,
            "DistancePips": 2.5,
            "MaxFutureTicks": 600,
            "MinConfidenceThreshold": 0.3,
            "MinScoreThreshold": 0.35
        },
        "OutputConfig": {
            "OutputPath": "output/features.parquet",
            "Format": "Parquet",
            "CompressOutput": true,
            "FlushInterval": 1000
        }
    },
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft": "Warning"
        }
    }
}